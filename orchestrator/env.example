# LLM Configuration
# Set LLM_PROVIDER to 'openai' or 'anthropic'
# For OpenAI (ChatGPT): Use 'openai' and your OpenAI API key
# For Anthropic (Claude): Use 'anthropic' and your Anthropic API key
LLM_PROVIDER=openai
LLM_API_KEY=sk-your-openai-api-key-here
LLM_MODEL=gpt-4

# Server Configuration
PORT=3001

# Orchestrator Configuration
AUTO_APPROVE=false
MAX_RETRIES=3
TIMEOUT=300000
